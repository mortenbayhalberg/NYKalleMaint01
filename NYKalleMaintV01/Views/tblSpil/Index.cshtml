    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CN_SpilDato)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CN_SpilSesson)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CN_SpilKalleKamel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CN_SpilPlacering)
        </td>
