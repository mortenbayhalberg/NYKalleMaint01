        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblSpiller.CN_SpillerForNavn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CN_SpilPK, URLspillerId = ViewBag.URLspillerId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CN_SpilPK, URLspillerId = ViewBag.URLspillerId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CN_SpilPK, URLspillerId = ViewBag.URLspillerId }) |
            @Html.ActionLink("Frame", "Index", "tblFrame", new { URLspillerId = ViewBag.URLspillerId, URLspilId = item.CN_SpilPK }, null )
            @* Jørn har vist mig denne: <a href="@Url.Action( "Index", "tblFrame", new { id=-1, URLspillerId = ViewBag.URLspillerId, URLspilId = item.CN_SpilPK } )">Frame</a>*@
        </td>
    </tr>
}

</table>
